{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Vite Debug in Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/frontend",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/src/*",
        "webpack:///*": "${webRoot}/*",
        "vite:///*": "${webRoot}/*"
      }
    }
  ]
}